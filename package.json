{
  "name": "@adamjkb/netlify-cms-widget-select-async",
  "version": "1.1.0",
  "description": "NetlifyCMS select widget from external resources",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adamjkb/netlify-cms-widget-select-async.git"
  },
  "scripts": {
    "build": "vite build --emptyOutDir",
    "deployment": "vite build --mode deployment",
    "dev:cms-proxy": "npx netlify-cms-proxy-server",
    "dev:vite": "vite",
    "dev": "npm-run-all --parallel dev:**",
    "serve": "vite preview"
  },
  "keywords": [
    "netlify",
    "netlify-cms",
    "widget"
  ],
  "author": {
    "name": "Adam Jakab",
    "email": "hello@adamjkb.com",
    "url": "https://adamjkb.com"
  },
  "license": "MIT",
  "devDependencies": {
    "dlv": "^1.1.3",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.23.2",
    "fuse.js": "^6.4.6",
    "immutable": "^3.8.2",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.23",
    "netlify-cms": "^2.10.130",
    "netlify-cms-lib-widgets": "^1.6.1",
    "netlify-cms-ui-default": "^2.13.0",
    "netlify-cms-widget-create-select": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react-immutable-proptypes": "^2.2.0",
    "react-select": "^3.0.0",
    "vite": "^2.3.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
